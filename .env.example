APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# File Upload Security Configuration
FILE_UPLOAD_VIRUS_SCAN=false
FILE_UPLOAD_ENHANCED_SCAN=true
FILE_UPLOAD_STRICT_MIME=true
FILE_UPLOAD_QUARANTINE=false
FILE_UPLOAD_MAX_COMPRESSED_SIZE=10485760
FILE_UPLOAD_BLOCK_EXECUTABLE=true
FILE_UPLOAD_SCAN_METADATA=true
FILE_UPLOAD_VALIDATE_STRUCTURE=true

# File Upload Processing
FILE_UPLOAD_AUTO_OPTIMIZE=true
FILE_UPLOAD_IMAGE_QUALITY=85
FILE_UPLOAD_MAX_WIDTH=1920
FILE_UPLOAD_MAX_HEIGHT=1080
FILE_UPLOAD_GENERATE_THUMBNAILS=true
FILE_UPLOAD_THUMBNAIL_WIDTH=300
FILE_UPLOAD_THUMBNAIL_HEIGHT=200

# File Upload Storage
FILE_UPLOAD_PUBLIC_DISK=public
FILE_UPLOAD_PRIVATE_DISK=local
FILE_UPLOAD_TEMP_DISK=local
FILE_UPLOAD_CLEANUP_TEMP=true
FILE_UPLOAD_TEMP_LIFETIME=24

# File Upload Rate Limiting
FILE_UPLOAD_RATE_LIMITING=true
FILE_UPLOAD_MAX_PER_MINUTE=10
FILE_UPLOAD_MAX_SIZE_PER_MINUTE=52428800
FILE_UPLOAD_MAX_PER_HOUR=100
FILE_UPLOAD_MAX_SIZE_PER_HOUR=524288000

# File Upload Logging
FILE_UPLOAD_LOG_UPLOADS=true
FILE_UPLOAD_LOG_FAILURES=true
FILE_UPLOAD_LOG_SECURITY=true
FILE_UPLOAD_LOG_QUARANTINE=true
FILE_UPLOAD_LOG_RATE_LIMITS=true

# File Upload Cleanup
FILE_UPLOAD_AUTO_CLEANUP_QUARANTINE=true
FILE_UPLOAD_QUARANTINE_RETENTION=30
FILE_UPLOAD_TEMP_CLEANUP_HOURS=24

# Performance Monitoring Configuration
ENABLE_PERFORMANCE_MONITORING=false
SLOW_REQUEST_THRESHOLD=1000
SLOW_QUERY_THRESHOLD=100
LOG_QUERIES=false

# Cache TTL Configuration (in seconds)
CACHE_TTL_SETTINGS=3600
CACHE_TTL_FLEET=300
CACHE_TTL_DASHBOARD=60
CACHE_TTL_AVAILABILITY=60
CACHE_TTL_REPORTS=3600
CACHE_TTL_CUSTOMERS=300

# Pagination Configuration
PAGINATION_DEFAULT=15
PAGINATION_MAX=100

# Lazy Loading Configuration
LAZY_LOAD_CHUNK_SIZE=100
ENABLE_CURSOR_PAGINATION=false

# Asset Optimization
CDN_URL=
ASSET_VERSIONING=true
MINIFY_HTML=false

# Database Optimization
DB_QUERY_CACHE=true
DB_POOL_SIZE=10
DB_STATEMENT_CACHE=100
